@model Project_63131631.Areas.Administrator.Models.ProductType_63131631

@{
    ViewBag.Title = "Tạo loại mới";
    Layout = "~/Areas/Administrator/Views/Shared/_Layout.cshtml";
}
<h2>Tạo loại mới</h2>

@using (Html.BeginForm("Create", "ProductType_63131631", FormMethod.Post))
{
